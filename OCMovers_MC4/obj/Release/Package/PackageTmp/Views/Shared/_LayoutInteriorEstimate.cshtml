						<li class=""><a href="@Url.RouteUrl("About")" title="ABOUT OLD CITY MOVERS">ABOUT</a></li>
							<li class="bar"></li>
                        <li><a href="@Url.RouteUrl("Services")" title="OLD CITY MOVERS SERVICES">SERVICES</a></li>
                        <li class="bar"></li>
                        <li><a href="@Url.RouteUrl("Contact")" title="CONTACT OLD CITY MOVERS">CONTACT US</a></li>

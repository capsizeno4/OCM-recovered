                </div>

            </div>

        </div>
        <div class="body">

            <div id="spinner">
                <div class="working">Sending your estimate...</div>
                <img src="/Content/themes/base/images/ajax-loader-bar.gif" />
            </div>

            @RenderBody()
        </div>
        <div class="footer"></div>
    </div>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="js/vendor/jquery-2.1.1.min.js"><\/script>')</script>

    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/jqueryval")

    <script src="//tinymce.cachefly.net/4.0/tinymce.min.js"></script>
    <script>

        $(document).ready(function () {
            tinymce.init({
                width: "100%",
                plugins: ["code fullscreen"],
                selector: "textarea:not(.mceNoEditor)",
                menubar: false,
                force_p_newlines: false,
                force_br_newlines: true,
                convert_newlines_to_brs: false,
                remove_linebreaks: true,
                cleanup: false,
                setup: function (ed) {
                    ed.on('init', function (args) {
                        tinymce.activeEditor.setContent("Add each room with the items under the room name");
                        this.getDoc().body.style.fontSize = '14px';
                    });
                },
                toolbar: "bold italic"
            });

        })
        

    </script>

    @Scripts.Render("~/bundles/main")

    @RenderSection("scripts", required: false)



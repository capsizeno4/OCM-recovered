<!doctype html>
<html lang="">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Old City Movers | A Philadelphia, PA insured & bonded moving company</title>
    <meta name="description" content="@ViewBag.Description">
    <meta name="keywords" content="@ViewBag.Keywords">
    <meta name="viewport" content="width=device-width">
    <link href='https://fonts.googleapis.com/css?family=Oswald:400,700,300' rel='stylesheet' type='text/css'>
    @*<link rel="stylesheet" href="/Content/normalize.min.css">*@
    @Styles.Render("~/bundles/css")
    <script src="https://use.fontawesome.com/721d17dcea.js"></script>
    <script src='https://www.google.com/recaptcha/api.js' async defer></script>

    <link rel="shortcut icon" href="/Content/themes/base/images/favicon.ico" type="image/x-icon">
    <link rel="icon" href="/Content/themes/base/images/favicon.ico" type="image/x-icon">
</head>
<body>
    <header class="navbar navbar-default">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar2">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar icon-bar-top"></span>
                    <span class="icon-bar icon-bar-middle"></span>
                    <span class="icon-bar icon-bar-bottom"></span>
                </button>
                <a class="navbar-brand" href="/">
                    <img src="~/Content/themes/base/images/icon-headerLogo.png" alt="Old City Movers">
                </a>
            </div>

            <nav id="navbar2" class="navbar-collapse collapse">
                <div class="navbar-cta pull-left">
                    <a href="~/Old-City-Movers-Estimate-Form" class="btn btn-primary">Get An Estimate</a>
                </div>
                <ul class="nav navbar-nav navbar-left">
                    <li><a href="/about">About</a></li>
                    <li><a href="/services">Services</a></li>
                    <li><a href="/contact">Contact Us</a></li>
                    <li><a href="/faqs">FAQs</a></li>
                </ul>

                <div class="navbar-testimonials">
                    <h3 class="text-caps">Find Us On</h3>
                    <ul class="list-inline">
                        <li><a href="http://philadelphia.citysearch.com/profile/44656396/philadelphia_pa/old_city_movers.html" target="_blank" title="OLD CITY MOVERS REVIEWS ON CITYSEARCH">Citysearch</a></li>
                        <li><a href="http://www.yelp.com/biz/old-city-movers-philadelphia" target="_blank" title="OLD CITY MOVERS REVIEWS ON YELP">Yelp</a></li>
                        <li><a href="http://www.angieslist.com/companylist/us/pa/philadelphia/old-city-movers-reviews-434879.aspx" title="Angie's List Super Service Award winner"><img src="~/Content/themes/base/images/ssaward_50.png"></a></li>
                    </ul>
                </div>


            </nav>
            <!--/.nav-collapse -->
        </div>
        <!--/.container-fluid -->
    </header>

    <div class="body">
        @RenderBody()
    </div>
    <footer class="page-footer">
        <div class="container">
            <div class="row">
                <div class="office col-xs-12 col-md-6">
                    <h4>Old City Movers</h4>
                    <p><strong>Office Hours</strong> Monday - Friday, 8am - 4pm</p>
                    <p><strong>Phone</strong> (267) 205-5209</p>
                    <p><strong>Email</strong> <a href="mailto:oldcitymovers@oldcitymovers.com">oldcitymovers@oldcitymovers.com</a></p>
                    <p><strong>Address</strong>2542 East Clearfield St., Apt C, Philadelphia, PA 19134</p>
                </div>
                <div class="menu col-xs-12 col-md-4 col-md-offset-2">
                    <ul class="list-unstyled">
                        <li><a class="grey-text text-lighten-3" href="~/about">About</a></li>
                        <li><a class="grey-text text-lighten-3" href="~/services">Services</a></li>
                        <li><a class="grey-text text-lighten-3" href="~/contact">Contact</a></li>
                        <li><a class="grey-text text-lighten-3" href="~/faqs">FAQ</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="footer-copyright">
            <div class="container">
                <div class="row">
                    <div class="col-xs-12 text-center">

                        @{
                            DateTime dt = DateTime.Now; 
                        }
                        	&#169;@dt.Year.ToString() Old City Movers
                    </div>
                </div>
            </div>
        </div>
    </footer>

    <script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
    <script> window.jQuery || document.write('<script src="js/vendor/jquery-2.1.1.min.js"><\/script>') </script>

    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/jqueryval")

    <script src="//cdn.tinymce.com/4/tinymce.min.js"></script>
    <script>
        tinymce.init({
            selector: 'textarea:not(.mceNoEditor)',
            width: 500,
            toolbar: [
                "undo redo | styleselect | bold italic | link image",
                "alignleft aligncenter alignright"
            ],
            //plugins: [ "code fullscreen" ]
        });

    </script>

    @Scripts.Render("~/bundles/main")

    @RenderSection("scripts", required: false)

    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r;
            i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date();
            a = s.createElement(o),
                m = s.getElementsByTagName(o)[0];
            a.async = 1;
            a.src = g;
            m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-62568807-1', 'auto');
        ga('send', 'pageview');

        $('html').on('blur', 'input[type="text"]', sumbitTextFieldGAEvent); 
        $('html').on('blur', 'textarea', sumbitTextFieldGAEvent);
        $('html').on('click', '.rbox', sumbitRadioFieldGAEvent);
        $('html').on('click', '.dropdown-menu a', sumbitDropdownFieldGAEvent);
        $('html').on('click', '.cbox', sumbitCheckboxFieldGAEvent);

        function sumbitCheckboxFieldGAEvent(e) {

            var val = $(this).closest('.slide').index() + ' : ' + $(this).parent().find('.gaCb').eq(0).attr('id')

            ga('send', {
                hitType: 'event',
                eventCategory: 'Estimate Form',
                eventAction: 'Answers',
                eventLabel: val
            });

            //console.log(val);
        }


        function sumbitDropdownFieldGAEvent(e) {

            var val = $(this).closest('.slide').index() + ' : ' + $(this).closest('.input.select').find('input[type="hidden"]').attr('id');

            ga('send', {
                hitType: 'event',
                eventCategory: 'Estimate Form',
                eventAction: 'Answers',
                eventLabel: val
            });

            //console.log(val);
        }

        function sumbitRadioFieldGAEvent(e) {

            var id = $(this).closest('.radio-group').find('input[type="hidden"]').attr('id');
            var val = $(this).closest('.slide').index() + ' : ' + id + ' ' + $(this).attr('id');

            ga('send', {
                hitType: 'event',
                eventCategory: 'Estimate Form',
                eventAction: 'Answers',
                eventLabel: val
            });

            //console.log(val);
        }

        function sumbitTextFieldGAEvent(e) {

            var val = $(this).closest('.slide').index() + ' : ' + $(this).attr('id')

            ga('send', {
                hitType: 'event',
                eventCategory: 'Estimate Form',
                eventAction: 'Answers',
                eventLabel: val
            });

            //console.log(val);
        }
    </script>
</body>
</html>
